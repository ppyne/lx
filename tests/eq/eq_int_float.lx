# non-strict equality (numeric coercion)

print(50 == 50.0);        print("\n");
print(50 == 50.000);      print("\n");
print(50 == 49.9999);     print("\n");

print(0 == 0.0);          print("\n");
print(-1 == -1.0);        print("\n");

# strict equality (type-sensitive)

print(50 === 50.0);       print("\n");
print(50.0 === 50.0);     print("\n");
print(50 === 50);         print("\n");

# non-equality

print(50 != 50.0);        print("\n");
print(50 !== 50.0);       print("\n");
