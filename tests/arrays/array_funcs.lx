$a = [1, 2, 3];
print(key_exists(1, $a) . "\n");
print(key_exists("x", $a) . "\n");
print(in_array(2, $a) . "\n");
print(in_array("2", $a) . "\n");
print(in_array("2", $a, false) . "\n");
print(in_array("x", $a) . "\n");

$vals = values(["a" => 1, "b" => 2, 5 => 3]);
print($vals[0] . "," . $vals[1] . "," . $vals[2] . "\n");

$b = [10, 20];
print(push($b, 30) . "\n");
print($b[2] . "\n");
print(pop($b) . "\n");
print(count($b) . "\n");
print(shift($b) . "\n");
print(count($b) . "\n");
print(unshift($b, 5) . "\n");
print($b[0] . "," . $b[1] . "\n");

$m = merge([1, 2], ["x" => 9, 5 => 10, 3]);
print($m[0] . "," . $m[1] . "," . $m[2] . "," . $m[3] . "," . $m["x"] . "\n");

$c = ["a" => 1, 2, "b" => 3, 4];
$s = slice($c, 1, 2);
print($s[0] . "," . $s["b"] . "\n");

$r = reverse([1, "a" => 2, 3]);
print($r[0] . "," . $r["a"] . "," . $r[1] . "\n");

$d = [1, 2, 3, 4];
$removed = splice($d, 1, 2, ["a" => 9, 8]);
print($removed[0] . "," . $removed[1] . "\n");
print($d[0] . "," . $d[1] . "," . $d[2] . "," . $d[3] . "\n");
