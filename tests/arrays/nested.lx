$a["x"]["y"] = 10;

print($a["x"]["y"]); print("\n");

# imbriqué avec int keys

$b = [];
$b[0] = [];
$b[0][1] = 42;

print($b[0][1]); print("\n");

# mélange string / int à plusieurs niveaux

$c = [];
