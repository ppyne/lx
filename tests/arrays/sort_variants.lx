print(join(values($b), ",") . "\n");

arsort($b);
print(join(keys($b), ",") . "\n");
print(join(values($b), ",") . "\n");

ksort($b);
print(join(keys($b), ",") . "\n");
print(join(values($b), ",") . "\n");

krsort($b);
print(join(keys($b), ",") . "\n");
print(join(values($b), ",") . "\n");
