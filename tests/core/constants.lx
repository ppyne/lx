# integer constants
if (LX_INT_SIZE == 4) {
    print("INT_SIZE_OK=true" . LX_EOL);
} else if (LX_INT_SIZE == 8) {
    print("INT_SIZE_OK=true" . LX_EOL);
}
print("ENDIANNESS=" . LX_ENDIANNESS); print(LX_EOL);
if (LX_INT_SIZE == 4) {
    print("INT_MAX_OK=" . (LX_INT_MAX == 2147483647) . LX_EOL);
    print("INT_MIN_OK=" . (LX_INT_MIN == -2147483648) . LX_EOL);
    print("INT_MODE_OK=true" . LX_EOL);
} else if (LX_INT_SIZE == 8) {
    print("INT_MAX_OK=" . (LX_INT_MAX == 9223372036854775807) . LX_EOL);
    print("INT_MIN_OK=" . (LX_INT_MIN == -9223372036854775808) . LX_EOL);
    print("INT_MODE_OK=true" . LX_EOL);
}

